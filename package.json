{
  "name": "ng2-starter",
  "version": "0.0.3",
  "scripts": {
    "watch:dev": "npm run server:dev & webpack --config webpack.config.ts --watch",
    "watch:prod": "npm run server:prod & webpack --config webpack.prod.config.ts --watch",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:aot": "rimraf aot",
    "clean:all": "npm run clean:dist && npm run clean:aot && npm run clean:coverage",
    "build": "webpack  --progress",
    "build:dev": "npm run build",
    "build:prod": "npm run clean:aot && npm run aot && npm run clean:dist && webpack --config webpack.prod.config.ts --progress",
    "aot": "ngc -p tsconfig.aot.json",
    "server:dev": "npm run build && nodemon dist/server/index.js",
    "server:prod": "npm run build:prod && nodemon dist/server/index.js",
    "start": "npm run server:dev",
    "pretest": "npm run clean:coverage",
    "test": "karma start"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/compiler-cli": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/http": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "@angular/platform-server": "~2.1.2",
    "@angular/router": "~3.1.2",
    "@angular/upgrade": "~2.1.2",
    "angular2-express-engine": "~2.1.0-rc.1",
    "angular2-platform-node": "~2.1.0-rc.1",
    "angular2-universal": "~2.1.0-rc.1",
    "angular2-universal-polyfills": "~2.1.0-rc.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "js.clone": "0.0.3",
    "methods": "~1.1.2",
    "morgan": "^1.7.0",
    "preboot": "~4.5.2",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.26",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/mime": "0.0.28",
    "@types/node": "^6.0.38",
    "@types/serve-static": "^1.7.27",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "cookie-parser": "^1.4.3",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.5",
    "inline-source-map": "^0.6.2",
    "istanbul-instrumenter-loader": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-remap-istanbul": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "nodemon": "^1.10.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.8",
    "remap-istanbul": "0.8.0",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "^1.0.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.3.0",
    "typescript": "2.0.10",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.1.0-beta.27",
    "webpack-bundle-analyzer": "1.4.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "2.1.0-beta.12",
    "webpack-merge": "~0.16.0"
  }
}
